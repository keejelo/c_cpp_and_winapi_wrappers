//---------------------------------------------------------------------------------------------
// ** Version.rc
//---------------------------------------------------------------------------------------------
// Add this file and "Version.h" to any project to have versioning, just compile, that's it!
//---------------------------------------------------------------------------------------------
// Source and inspiration:
// https://www.zachburlingame.com/2011/02/versioning-a-native-cc-binary-with-visual-studio/
//---------------------------------------------------------------------------------------------
#include "Version.h"
//---------------------------------------------------------------------------------------------


//---------------------------------------------------------------------------------------------
// Version resource block - DO NOT EDIT!
//---------------------------------------------------------------------------------------------

VS_VERSION_INFO VERSIONINFO
 FILEVERSION        VER_FILE_VERSION
 PRODUCTVERSION     VER_PRODUCT_VERSION
 FILEFLAGSMASK      0x3fL
 FILEFLAGS          VER_FILEFLAGS
 FILEOS             VER_FILEOS
 FILETYPE           VER_FILETYPE
 FILESUBTYPE        0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "Comments",         VER_COMMENTS_STR "\0"
            VALUE "LegalCopyright",   VER_COPYRIGHT_STR "\0"
            VALUE "CompanyName",      VER_COMPANY_NAME_STR "\0"
            VALUE "FileDescription",  VER_FILE_DESCRIPTION_STR "\0"
            VALUE "FileVersion",      VER_FILE_VERSION_STR "\0"
            VALUE "ProductVersion",   VER_PRODUCT_VERSION_STR "\0"
            VALUE "InternalName",     VER_INTERNAL_NAME_STR "\0"
            VALUE "OriginalFilename", VER_ORIGINAL_FILENAME_STR "\0"
            VALUE "ProductName",      VER_PRODUCTNAME_STR
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
